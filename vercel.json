{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0",
      "maxDuration": 30
    }
  },
  "env": {
    "SMTP_HOST": "@smtp_host",
    "SMTP_PORT": "@smtp_port",
    "SMTP_SECURE": "@smtp_secure",
    "SMTP_USER": "@smtp_user",
    "SMTP_PASS": "@smtp_pass",
    "RECIPIENT_EMAIL": "@recipient_email",
    "CC_EMAIL": "@cc_email"
  }
}
